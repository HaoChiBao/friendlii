{
    "manifest_version": 3,
    "name": "Friendlii",
    "description": "...",
    "version": "0.0.1",

    "permissions": [
        "scripting",
        "activeTab",
        "storage",
        "downloads"
    ],
    "host_permissions": ["<all_urls>"],
    "web_accessible_resources":[{
        "resources": ["/app/*"],
        "matches": ["<all_urls>"]
    }],
    "action": {
        "default_popup": "./app/popup.html"
    }
}